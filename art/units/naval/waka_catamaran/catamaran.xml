<animfile>
    <definebone>bone_boat_wake</definebone>
    <definebone>bone_master</definebone>
    <definebone>bone_canoe</definebone>
    <definebone>bone_hitpointbar</definebone>
    <definebone>bone_launchpoint</definebone>
    <definebone>bone_flag_civ</definebone>
    <definebone>bone_garrisonflag</definebone>
    <attachment>Fishing_net
        <component>fishing_net
            <assetreference type="GrannyModel">
                <file>units\attachments\fishing_net</file>
            </assetreference>
        </component>
        <anim>Idle
            <assetreference type="GrannyAnim">
                <file>units\attachments\fishing_net_caravel</file>
            </assetreference>
            <component>fishing_net</component>
        </anim>
    </attachment>
    <attachment>ClickZone
        <component>ClickZone
            <assetreference type="GrannyModel">
                <file>units\naval\waka_catamaran\catamaran_shape_Model</file>
            </assetreference>
        </component>
        <anim>None
            <component>ClickZone</component>
        </anim>
    </attachment>
    <attachment>IdleWaterFoam
        <component>Idlefoam
            <assetreference type="popcornFx">
                <file>water_effects\idle_waterFoam_medium.pkfx</file>
            </assetreference>
        </component>
        <anim>Idle
            <component>Idlefoam</component>
        </anim>
    </attachment>
    <attachment>MoveWaterFoam
        <component>Movefoam
            <assetreference type="popcornFx">
                <file>water_effects\move_waterFoam_medium.pkfx</file>
            </assetreference>
        </component>
        <anim>None
            <component>Movefoam</component>
        </anim>
    </attachment>
    <attachment>DeathWaterFoam
        <component>Deathfoam
            <assetreference type="popcornFx">
                <file>water_effects\death_waterFoam_medium.pkfx</file>
            </assetreference>
        </component>
        <anim>None
            <component>Deathfoam</component>
        </anim>
    </attachment>
    <attachment>Death_EXP
        <component>exp
            <assetreference type="popcornFx">
                <file>units\spc\fire_ship\Exp_ship.pkfx</file>
            </assetreference>
        </component>
        <anim>None
            <component>exp</component>
        </anim>
    </attachment>
    <attachment>Death_EXP_Wood
        <component>exp
            <assetreference type="popcornFx">
                <file>projectiles\ship_debris_wood.pkfx</file>
            </assetreference>
        </component>
        <anim>None
            <component>exp</component>
        </anim>
    </attachment>
    <component>ModelComp
         <assetreference type="GrannyModel">
            <file>units\naval\waka_catamaran\catamaran_Model</file>
        </assetreference>
        <decal>
            <effecttype>default</effecttype>
            <texture>shadows_selections\shadow_circle_128x128</texture>
            <selectedtexture>shadows_selections\selection_oval_64x128</selectedtexture>
            <width>7.00</width>
            <height>11.00</height>
        </decal>
        <watersplash>
            <texture>units\naval\colony_ship\colony_ship_hullmask</texture>
            <width>10.0</width>
            <height>10.0</height>
        </watersplash>
        <attach a="ClickZone" frombone="ATTACHPOINT" tobone="ATTACHPOINT" syncanims="1"></attach>
    </component>
    <anim>Idle
        <assetreference type="GrannyAnim">
            <file>units\naval\waka_catamaran\catamaran_idle_Anim</file>
            <transitiontime>0.200</transitiontime>
        </assetreference>
        <component>ModelComp</component>
        <attach a="IdleWaterFoam" frombone="ATTACHPOINT" tobone="ATTACHPOINT" syncanims="0"></attach>
    </anim>
    <anim>Walk
        <assetreference type="GrannyAnim">
            <file>units\naval\waka_catamaran\catamaran_move_Anim</file>
            <transitiontime>0.200</transitiontime>
        </assetreference>
        <component>ModelComp</component>
        <attach a="MoveWaterFoam" frombone="ATTACHPOINT" tobone="ATTACHPOINT" syncanims="0"></attach>
    </anim>
    <anim>Death
        <assetreference type="GrannyAnim">
            <file>units\naval\waka_canoe\waka_death_Anim</file>
            <tag type="SpecificSoundSet" checkvisible="1" set="DudeHitsWater">0.43</tag>
        </assetreference>
        <component>ModelComp</component>
        <attach a="DeathWaterFoam" frombone="MASTER" tobone="MASTER" syncanims="0"></attach>
    </anim>
    <anim>BombardAttack
        <assetreference type="GrannyAnim">
            <file>animation_library\naval\tlaloc_canoe\idle</file>
            <tag type="Attack">0.02</tag>
            <tag type="SpecificSoundSet" checkvisible="1" set="FalconetShoot">0.08</tag>
            <tag type="Particles" particlename="smallcannonmuzzle">0.08</tag>
            <tag type="Particles" particlename="BattleField_Smoke_Small">0.08</tag>
            <tag type="WindEffect" effectname="SmallArtilleryFire" effectbone="bone_launchpoint">0.08</tag>
        </assetreference>
        <component>ModelComp</component>
    </anim>
    <anim>GatherFish
        <assetreference type="GrannyAnim">
            <file>units\naval\waka_catamaran\catamaran_idle_Anim</file>
            <tag type="SpecificSoundSet" checkvisible="1" set="SplashSmall">0.89</tag>
        </assetreference>
        <component>ModelComp</component>
        <attach a="Fishing_net" frombone="MASTER" tobone="MASTER" syncanims="0"></attach>
    </anim>
    <anim>RangedIdle
        <assetreference type="GrannyAnim">
            <file>animation_library\naval\tlaloc_canoe\idle</file>
            <transitiontime>0.200</transitiontime>
        </assetreference>
        <component>ModelComp</component>
        <attach a="IdleWaterFoam" frombone="ATTACHPOINT" tobone="ATTACHPOINT" syncanims="0"></attach>
    </anim>
</animfile>