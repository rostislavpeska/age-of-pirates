  <submodel>construction stage3
    <attachment>scaffolding
      <component>construction3
        <assetreference type="GrannyModel">
          <file>buildings\constructions\3x3_stage3</file>
        </assetreference>
        <decal>
          <effecttype>default</effecttype>
          <texture>buildings\saloon\Saloon_floor_00</texture>
          <selectedtexture>shadows_selections\selection_square_128x128</selectedtexture>
          <width>8.00</width>
