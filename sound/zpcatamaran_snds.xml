          <soundset name="Maori_Soldier_Attack">
          </soundset>
        </choice>
      </targetlogic>
    </soundtype>
    <soundtype name="Exists">
      <soundset name="AmbienceShip">
      </soundset>
    </soundtype>
  </protounit>
