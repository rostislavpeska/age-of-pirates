                                 <Setter.Value>
                                    <ControlTemplate TargetType="{x:Type ListViewItem}">
                                       <Border Background="{TemplateBinding Background}">
                                          <GridViewRowPresenter Content="{TemplateBinding Content}" Margin="0,5,0,5" />
                                       </Border>
                                    </ControlTemplate>
                                 </Setter.Value>
                              </Setter>

                              <Style.Triggers>
                                 <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Background" Value="{StaticResource button_filemanager_hover_fill}" />
                                 </Trigger>
                                 <Trigger Property="IsSelected" Value="True">
                                    <Setter Property="Background" Value="{StaticResource button_filemanager_selected_fill}" />
                                 </Trigger>
                                 <EventTrigger RoutedEvent="PreviewMouseDown">
                                    <SoundPlayerAction Source="pack://siteoforigin:,,,/resources/audio/smallbutton.wav" />
